{% extends 'base.html.twig' %}

{% block body %}
    <h1>UserLike</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ userLike.id }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if userLike.createdAt %}{{ userLike.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ userLike.userId }}</td>
            </tr>
            <tr>
                <th>Catelogueid</th>
                <td>{{ userLike.catelogueId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('userlike_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('userlike_edit', { 'id': userLike.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
